#: ../includes/elements/svg.php:50
msgid "Code"
msgstr "Codi"

#: ../includes/admin/admin-screen-settings.php:1226
msgid "Disable global classes"
msgstr "Desactiva les classes globals"

#: ../includes/admin/admin-screen-settings.php:1226
msgid "Interface"
msgstr "Interfície"

#: ../includes/admin/admin-screen-settings.php:1231
msgid "Variable dropdown: Hide value"
msgstr "Menú desplegable de variables: amaga el valor"

#: ../includes/admin/admin-screen-settings.php:1236
msgid "Code control"
msgstr "Control de codi"

#: ../includes/admin/admin-screen-settings.php:1236,
#: ../includes/elements/nav-nested.php:908,
#: ../includes/elements/nav-nested.php:935,
#: ../includes/elements/offcanvas.php:138, ../includes/elements/toggle.php:13,
